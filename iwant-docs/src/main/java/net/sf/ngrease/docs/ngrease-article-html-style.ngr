# ngrease 1

context {

transformers {

#:"TODO polish descript so we don't need to put this in an ngrease package"
article {
  additional-head-element-list:
    html-fragment {
      link {rel:"stylesheet" href:"style.css"}
      link {rel:"icon" href:"favicon.ico"}
      link {rel:"shortcut icon" href:"favicon.ico"}
    }
  body: $:quote:
    body {
      $(@):transform {to:content-element-list from:$:source}
    }
}

}

}
